METHOD VERRNC( ) 
LOCAL oRNC AS USEREDE	
LOCAL aDAD AS ARRAY
LOCAL nRNC AS DWORD
LOCAL oJAN AS JRNC4
IF ! entramenu("TGQ",10)
	RETU SELF
ENDIF	
nRNC:=SELF:server:FIELDGET("RNC")
aDAD:={zCURINI,"RNC.DBF",ZCURDIR}
oRNC:=USEREDE{aDAD}
IF oRNC:nERRO#0
   RETU SELF
ENDIF
oRNC:GOTOP()
IF oRNC:Seek(nRNC)
   oJAN:=JRNC4{SELF,,oRNC}
   oJAN:SHOW()
ELSE
   alert("Rnc Não Encontrada")
ENDIF	
oRNC:CLOSE()
	

