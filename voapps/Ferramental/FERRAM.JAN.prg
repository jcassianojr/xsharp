return FERRAM := uValue


METHOD Incluir( ) 
LOCAL cDESENHO AS STRING
cDESENHO:=SELF:SERVER:FIELDGET("FERRAM")
SELF:oSFJFERRAMI:SERVER:SUSPENDNOTIFICATION()
SELF:oSFJFERRAMI:SERVER:APPEND()
SELF:oSFJFERRAMI:SERVER:FIELDPUT("FERRAM",cDESENHO)
SELF:oSFJFERRAMI:SERVER:commit()
SELF:oSFJFERRAMI:SERVER:resetnotification()
SELF:oSFJFERRAMI:SERVER:notify(notifyappend)
SELF:oSFJFERRAMI:Browser:REFRESH()	

